{
    "name": "drone076/redis-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 4 Redis Bundle",
    "keywords": ["redis", "symfony", "bundle", "symfony 4"],
    "homepage": "https://github.com/drone076/redis-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dr One",
            "email": "drone076@yahoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\RedisBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.1.3",
        "predis/predis": "~1.1",
        "symfony/framework-bundle": "^3.4||^4.0",
        "symfony/yaml": "^3.4||^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpunit/php-code-coverage": "^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
